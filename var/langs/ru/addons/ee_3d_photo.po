msgctxt "SettingsTooltips::ee_3d_photo::ee_3d_photo_active"
msgid "ee_3d_photo_active_tooltips"
msgstr "Подключить все необходимые скрипты и вывести 3D картинки если они загружены."

msgctxt "SettingsOptions::ee_3d_photo::ee_3d_photo_show_button"
msgid "ee_3d_photo_show_button"
msgstr "Кнопки плеера"

msgctxt "SettingsTooltips::ee_3d_photo::ee_3d_photo_show_button"
msgid "ee_3d_photo_show_button_tooltips"
